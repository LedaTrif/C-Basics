using System;

namespace _04.InchesAndCm
{
    class Program
    {
        static void Main(string[] args)
        {

            //Read real number from input
            double inches = double.Parse(Console.ReadLine());

            //Convert to cm 
            double cm = inches * 2.54;

            //Show result
            Console.WriteLine(cm);
        }
    }
}

